pcm.!default {
	type pulse
    #type plug
	#slave.pcm "dmixer"
    hint.description "Default Audio Device"
}
ctl.!default {
	type pulse
}

pcm.pulse {
    type pulse
}

ctl.pulse {
    type pulse
}

pcm.dmixer  {
 	type dmix
 	ipc_key 1024
 	slave {
        pcm "hw:0,3"
        #pcm "hw:1,0"
        period_time 0
        period_size 1024
        buffer_size 4096
        rate 44100
	}
	bindings {
		0 0
		1 1
	}
}

ctl.dmixer {
	type hw
	card 1
}

pcm.analog-hw {
    type hw
    card 1
    device 0
}

pcm.digital-hw {
    type hw
    card 0
    device 3
}
